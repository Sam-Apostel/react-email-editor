{
  "include": ["src"],
  "compilerOptions": {
    "target": "es2018",
    "module": "commonjs",
    "lib": ["es2018", "dom"],
    "declaration": true,
    "sourceMap": true,
    "baseUrl": "src",
    "strict": true,
    "moduleResolution": "node",
    "jsx": "react",
    "esModuleInterop": true,
    "noEmit": true,
  }
}
